{
  "name": "@reimagined/github-actions-deploy",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "rm -r dist && ncc build src/main-entry.ts -o dist/main --license lic.txt && ncc build src/post-entry.ts -o dist/post --license lic.txt",
    "compile": "tsc --build",
    "test": "jest --coverage --config=../jest.config.js",
    "clean": "rm -rf dist node_modules coverage lib",
    "add-dist": "git add --force dist"
  },
  "dependencies": {
    "@actions/core": "1.2.5",
    "semver": "7.3.4",
    "latest-version": "5.1.0",
    "lodash.set": "4.3.2"
  },
  "devDependencies": {
    "@types/semver": "7.3.4",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.set": "4.3.6",
    "jest": "26.6.3",
    "ts-jest": "26.5.1",
    "lodash.omit": "4.5.0"
  }
}

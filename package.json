{
  "name": "github-actions",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "validate-lock-file": "ts-node validate-lock-file.ts",
    "prettier": "prettier --write \"**/**.{ts,tsx,js,json}\"",
    "prettier:check": "prettier --list-different \"**/**.{ts,tsx,js,json}\"",
    "clean": "yarn workspaces run clean && rm -rf node_modules yarn.lock",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "build": "yarn workspaces run build",
    "add-dist": "yarn workspaces run add-dist",
    "release": "ts-node release.ts",
    "test": "yarn workspaces run test"
  },
  "devDependencies": {
    "@types/node": "14.14.28",
    "@types/semver": "7.3.4",
    "@types/yarnpkg__lockfile": "1.1.4",
    "@yarnpkg/lockfile": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-import": "2.20.2",
    "prettier": "2.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.3.2",
    "consola": "2.15.3",
    "semver": "7.3.4"
  },
  "workspaces": [
    "common",
    "publish",
    "install-cloud",
    "deploy",
    "git-release",
    "release-pr-bot",
    "generate-changelog",
    "resolve-gate"
  ]
}
